<glade-catalog name="coverart_widgets" library="gladepython" domain="glade-3" 
               depends="gtk+" version="1.0">
  <init-function>glade_python_init</init-function>
  <glade-widget-classes>
    <glade-widget-class title="PopupButton" name="PopupButton" 
                        generic-name="PopupButton" parent="GtkButton"
                        icon-name="widget-gtk-button"/>
    <glade-widget-class title="MenuButton" name="MenuButton" 
                        generic-name="MenuButton" parent="GtkButton"
                        icon-name="widget-gtk-button"/>
    <glade-widget-class title="ImageToggleButton" name="ImageToggleButton" 
                        generic-name="ImageToggleButton" parent="GtkButton"
                        icon-name="widget-gtk-button"/>
    <glade-widget-class title="ImageRadioButton" name="ImageRadioButton" 
                        generic-name="ImageRadioButton" parent="GtkRadioButton"
                        icon-name="widget-gtk-radiobutton"/>
    <glade-widget-class title="RBSearchEntry" name="RBSearchEntry" 
                        generic-name="RBSearchEntry" parent="GtkHBox"
                        icon-name="widget-gtk-box"/>
    <glade-widget-class title="SearchEntry" name="SearchEntry" 
                        generic-name="SearchEntry" parent="RBSearchEntry"
                        icon-name="widget-gtk-box"/>
    <glade-widget-class title="QuickSearchEntry" name="QuickSearchEntry" 
                        generic-name="QuickSearchEntry" parent="GtkBox"
                        icon-name="widget-gtk-box"/>
    <glade-widget-class title="ProxyPopupButton" name="ProxyPopupButton" 
                        generic-name="ProxyPopupButton" parent="GtkFrame"
                        icon-name="widget-gtk-frame"/>
    <glade-widget-class title="EnhancedIconView" name="EnhancedIconView" 
                        generic-name="EnhancedIconView" parent="GtkIconView"
                        icon-name="widget-gtk-iconview"/>
    <glade-widget-class title="CoverIconView" name="CoverIconView" 
                        generic-name="CoverIconView" parent="GtkIconView"
                        icon-name="widget-gtk-iconview"/>
    <glade-widget-class title="ArtistView" name="ArtistView" 
                        generic-name="ArtistView" parent="GtkTreeView"
                        icon-name="widget-gtk-treeview"/>
    <glade-widget-class title="PanedCollapsible" name="PanedCollapsible"
                        generic-name="PanedCollapsible" parent="GtkPaned"
                        icon-name="widget-gtk-paned"/>
  </glade-widget-classes>

   <glade-widget-group name="python" title="Python">
     <glade-widget-class-ref name="PopupButton"/>
     <glade-widget-class-ref name="MenuButton"/>    
     <glade-widget-class-ref name="ImageToggleButton"/>
     <glade-widget-class-ref name="ImageRadioButton"/>
     <glade-widget-class-ref name="RBSearchEntry"/>
     <glade-widget-class-ref name="SearchEntry"/>
     <glade-widget-class-ref name="QuickSearchEntry"/>
     <glade-widget-class-ref name="ProxyPopupButton"/>
     <glade-widget-class-ref name="EnhancedIconView"/>
     <glade-widget-class-ref name="PanedCollapsible"/>
   </glade-widget-group>
</glade-catalog>
